{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "name": {
      "type": "string"
    },
    "display_name": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "dependencies": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "path": {
            "type": "string"
          },
          "ref": {
            "type": "string"
          },
          "custom_url": {
            "type": "string"
          }
        }
      }
    }
  },
  "required": [
    "name",
    "display_name",
    "description"
  ]
}
